<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlController.FXMLCadProfessorController">
   <children>
      <Label fx:id="textTitle" alignment="CENTER" contentDisplay="CENTER" layoutX="183.0" layoutY="14.0" text="Cadastro de Professor" wrapText="true">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <HBox layoutX="-4.0" layoutY="49.0" prefHeight="349.0" prefWidth="600.0" spacing="60.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0">
         <children>
            <VBox prefHeight="353.0" prefWidth="250.0">
               <children>
                  <Label text="Nome">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txfNome" />
                  <Label text="Cpf">
                     <padding>
                        <Insets top="20.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txfCpf" />
                  <Label text="Telefone">
                     <padding>
                        <Insets top="20.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txfTelefone" />
                  <Label text="Endereço">
                     <padding>
                        <Insets top="20.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txfEndereco" prefHeight="54.0" prefWidth="240.0" />
               </children>
            </VBox>
            <VBox prefHeight="349.0" prefWidth="250.0">
               <children>
                  <Label text="Salario">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txfSalario" />
                  <Label text="Titulação">
                     <padding>
                        <Insets top="20.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txfTitulo" />
                  <Label text="Área de Pesquisa">
                     <padding>
                        <Insets top="20.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txfAPesquisa" />
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                     <children>
                        <Button fx:id="btnReturn" mnemonicParsing="false" onAction="#btReturn" text="Voltar" />
                        <Button fx:id="btnSave" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#btSave" text="Salvar" />
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets top="60.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
